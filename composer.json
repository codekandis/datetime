{
	"name": "codekandis/datetime",
	"description": "`codekandis/datetime` is a library providing `DateTime` constants, interfaces and classes.",
	"version": "dev-development",
	"keywords": [
		"php",
		"datetime",
		"date",
		"time",
		"timestamp",
		"constants",
		"interfaces",
		"classes"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/checklist/issues",
		"docs": "https://github.com/codekandis/checklist/README.md",
		"source": "https://github.com/codekandis/checklist/tree/master"
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	},
	"require": {
		"php": "^7.4"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\DateTime\\": [
				"src"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\DateTime\\Tests\\": [
				"tests"
			]
		}
	}
}
